<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="ProductPackagingUnitType">
        <property name="name" dataBuilderRule="=packaging_unit_type.test.name"/>
    </transfer>

    <transfer name="ProductPackagingUnitAmount">
        <property name="isAmountVariable" dataBuilderRule="boolean(100)"/>
        <property name="defaultAmount" dataBuilderRule="randomFloat()"/>
    </transfer>

</transfers>
